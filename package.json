{
  "name": "reluzent-ui",
  "version": "1.0.0",
  "description": "Reluzent ui components",
  "author": "contato@reluzent.com.br",
  "license": "MIT",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.11.1 <=8.11.4"
  },
  "private": true,
  "scripts": {
